<section class="spark-gallery {{ spark_class }}">
	{% if header %}<h2>{{ header }}</h2>{% endif %}
	<ul class="slides">
	{% for post in feed %}
		<li class="post-item">
			<a href="{{ post.permalink }}">
				<span class="date">{{ post.date }}</span> |
				 <span class="title">{{ post.title }}</span>
			</a>
		</li>
	{% endfor %}
	</ul>
</section>